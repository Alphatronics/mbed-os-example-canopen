{
    "macros": [

    ],
    "target_overrides": {
        "*": {
            "platform.stdio-baud-rate": 115200,
            "platform.default-serial-baud-rate": 115200,
            "platform.stdio-convert-newlines": true
        },
        "LPC1768": {
            "target.features_remove": ["NANOSTACK", "BLE", "LWIP"]
        },
        "NUCLEO_F091RC": {
            "target.features_remove": ["NANOSTACK", "BLE", "LWIP"],
            "target.components_add": [
                "DATAFLASH"
            ],
            "dataflash.binary-size": true,
            "dataflash.SPI_MOSI": "PB_5",
            "dataflash.SPI_MISO": "PB_4",
            "dataflash.SPI_CLK":  "PB_3",
            "dataflash.SPI_CS":   "PA_8"
        }
    }
}
